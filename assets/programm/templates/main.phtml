        </div>
        <div class="vertical-wrap">
            <div class="vertical-lines-subtitle lines-subtitle">
                <div class="h2">[[*title_section_11]]</div>
                <div class="cutter">
                    <div><span><i>[[*title_section_11]]</i></span></div>
                    <div><span><i>[[*title_section_11]]</i></span></div>
                    <div><span><i>[[*title_section_11]]</i></span></div>
                    <div><span><i>[[*title_section_11]]</i></span></div>
                </div>
            </div>
        </div>
        <div class="interest-wrap">
            <h2 class="interest-head">[[*interest-title]]</h2>
            <div class="wrap-interest-list">
                [[!getImageList?
                &docid=`[[*id]]`
                &tvname=`migx-interest-slider`
                &tpl=`@CODE:
                <div class="interest-item">
                    <img src="[[+image]]" alt="[[+text]]">
                    <div class="interest-text">[[+text]]</div>
                </div>
                `
                ]]
            </div>
        </div>
    </div>
</section>

