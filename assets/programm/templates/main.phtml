</section><!--9-->
<section id="work-graduates" class="section10 work grey-section"><!--10-->
    <div class="section-wrap">
        <div class="vertical-wrap">
            <div class="vertical-lines-subtitle lines-subtitle">
                <div class="h2">[[*title_section_10]]</div>
                <div class="cutter">
                    <div><span><i>[[*title_section_10]]</i></span></div>
